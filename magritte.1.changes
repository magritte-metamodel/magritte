dValuesDo: [ :key :value |		value isNil			ifTrue: [ result at: key put: key default yourself ] ].	^ result! !!MAMemento methodsFor: 'private' stamp: 'CompatibleUserName 6/18/2020 21:02:28'!pullRaw	| result |	result := Dictionary new.	self magritteDescription do: [ :each |		result at: each put: (self model			readUsing: each) ].	^ result! !!MAMemento methodsFor: 'private' stamp: 'CompatibleUserName 6/18/2020 21:02:28'!push: aDictionary	"Utility method to push a dictionary mapping descriptions to values into the model."	aDictionary keysAndValuesDo: [ :key :value |		(key isVisible and: [ key isReadonly not ]) 			ifTrue: [ self model write: value using: key ] ]! !!MAMemento methodsFor: 'private' stamp: 'CompatibleUserName 6/18/2020 21:02:28'!readUsing: aDescription	^ self subclassResponsibility! !!MAMemento methodsFor: 'actions' stamp: 'CompatibleUserName 6/18/2020 21:02:28'!reset	"Reset the memento from the model."! !!MAMemento methodsFor: 'initialization' stamp: 'CompatibleUserName 6/18/2020 21:02:28'!setDescription: aDescription	description := aDescription! !!MAMemento methodsFor: 'initialization' stamp: 'CompatibleUserName 6/18/2020 21:02:28'!setModel: aModel	model := aModel! !!MAMemento methodsFor: 'actions' stamp: 'CompatibleUserName 6/18/2020 21:02:28'!validate	"Check if the data in the receiver would be valid if committed. In case of problems an exception is raised."	self magritteDescription validate: self! !!MAStraightMemento class methodsFor: 'testing' stamp: 'CompatibleUserName 6/18/2020 21:02:28'!isAbstract	^ false! !!MAStraightMemento methodsFor: 'private' stamp: 'CompatibleUserName 6/18/2020 21:02:28'!readUsing: aDescription	^ (self model readUsing: aDescription)		ifNil: [ aDescription default ]! !!MAStraightMemento methodsFor: 'private' stamp: 'CompatibleUserName 6/18/2020 21:02:28'!write: anObject using: aDescription	self model write: anObject using: aDescription! !!MAObject class methodsFor: 'testing' stamp: 